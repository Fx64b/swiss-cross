{
  "name": "swiss-cross",
  "version": "1.0.1",
  "description": "A React component for rendering the Swiss cross symbol",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "homepage": "https://fx64b.dev/projects/swiss-cross",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && tsc && tsc --module esnext --outDir dist/esm && node scripts/build-esm.js",
    "prepublishOnly": "pnpm build",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">=18.12.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "react",
    "component",
    "swiss",
    "switzerland",
    "cross",
    "flag",
    "symbol"
  ],
  "author": "Fx64b (https://fx64b.dev)",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "semantic-release": "^24.2.7",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fx64b/swiss-cross.git"
  }
}
